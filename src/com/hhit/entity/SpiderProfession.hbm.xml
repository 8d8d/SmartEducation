<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hhit.entity">

	<class name="SpiderProfession" table="T_SpiderProfession">
		<id name="id" type="int">
			<column name="Id"></column>
            <generator class="native"/>
		</id>
		<property name="professionName" type="string">	
			<column name="ProfessionName" length="50">
				<comment>专业名称</comment>
			</column>
		</property>
		<property name="courseCount" type="integer">
			<column name="CourseCount">
				<comment>课程数量</comment>
			</column>
		</property>
		<property name="url" type="string">
			<column name="Url">
				<comment>url地址</comment>
			</column>
		</property>
		<property name="professionType" type="string">
			<column name="ProfessionType" length="50">
				<comment>专业类型</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
